{
	"emmet.showSuggestionsAsSnippets": true,
	"editor.snippetSuggestions": "top",
	"javascript.format.enable": false,
	"editor.tabSize": 2,
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	]
}
